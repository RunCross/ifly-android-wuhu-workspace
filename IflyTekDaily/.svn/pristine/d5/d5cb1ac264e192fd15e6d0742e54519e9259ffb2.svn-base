            android:name="com.firstgroup.iflytekdaily.main.DailyMainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.firstgroup.iflytekdaily.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.firstgroup.iflytekdaily.work.LoginMainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name="com.firstgroup.iflytekdaily.menu.BottomMenu" >
           
        </activity>
        <activity 
            android:name="com.firstgroup.iflytekdaily.main.ChangePassword"
            >
             <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>