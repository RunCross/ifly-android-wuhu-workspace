<?xml version="1.0" encoding="utf-8"?>

<!-- 旋转 -->
<!-- fromDegrees从几度转到几度，pivotX原点坐标 -->
<rotate
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="0"
    android:toDegrees="360"
    android:pivotX="5"
    android:pivotY="5"
    android:duration="2000"
    android:fillAfter="false"
>
</rotate>
